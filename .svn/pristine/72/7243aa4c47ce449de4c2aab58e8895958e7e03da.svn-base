/*------------------------------------------------------------------------------- CARRUSEL-*/
#carrousel {
    overflow:hidden;
    padding: 0 20px;
}

.innerCarrousel {
    overflow:hidden;
}

.innerCarrousel ul{
    width:100%;
    overflow:hidden;
    margin-top: 24px;
    height:auto;
    position:relative;
    transform: translate3d(0, 0, 0);
    -webkit-perspective: 1000;
    -webkit-backface-visibility: hidden;
    transform:  translate3d(0, 0, 0);
    -webkit-transform:  translate3d(0, 0, 0);
    -moz-transform:  translate3d(0, 0, 0);
    
}

.transition {
    -webkit-transition: all 300ms ease-in-out;
    -moz-transition: all 300ms ease-in-out;
    -ms-transition: all 300ms ease-in-out;
    -o-transition: all 300ms ease-in-out;
}

.transform {
    transform:  translate3d(0, 0, 0);
    -webkit-transform:  translate3d(0, 0, 0);
    -moz-transform:  translate3d(0, 0, 0);
}

.innerCarrousel ul li{
    margin-bottom: 20px;
    margin-top: 15px;
    position: relative;
    opacity: 0.8;
}

.innerCarrousel ul li.current{
    border: 4px solid #2C9AD0;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -o-border-radius: 5px;
    -moz-border-radius:5px;
    margin-top:0px; 
    -webkit-box-shadow:  0px 5px 30px 0px rgba(0, 0, 0, 0.3);
    -moz-box-shadow:  0px 5px 30px 0px rgba(0, 0, 0, 0.3);
    box-shadow:  0px 5px 30px 0px rgba(0, 0, 0, 0.3);
    opacity:1.0;
/*    -webkit-transition: all 300ms ease-in-out;
    -moz-transition: all 300ms ease-in-out;
    -ms-transition: all 300ms ease-in-out;
    -o-transition: all 300ms ease-in-out;*/
}

.innerCarrousel ul li img{
    display:block;
}

.fichaTitle{
    position: absolute;
    bottom: 0;
    background: url('../img/tablet/transparentBG.png') repeat;
    width: 100%;
    padding: 10px 0;
}
/*------------------------------------------------------------------------------- BUSCADOR-*/
#buscador{
    display: none;
    border-radius: 20px;
    -webkit-border-radius: 20px;
    -o-border-radius: 20px;
    -moz-border-radius: 20px;
    -ms-border-radius: 20px;
    width: 300px;
    height: 25px;
    padding: 5px 10px;
    border: 0 none;
    margin-right: 10px;
    background: url('../img/tablet/lupaActive.png') no-repeat 95% center #fff;
    font-size: 16px;
}

.searchContent {
    width:790px;
    border-radius: 20px;
    -webkit-border-radius: 20px;
    -o-border-radius: 20px;
    -moz-border-radius: 20px;
    -ms-border-radius: 20px;
    border: 3px solid #fff;
    height:390px;
    background:#1e1e1e;
    position:absolute;
    left: 50%;
    display:none;
    top:100px;
    z-index:99999;
}

.innerSearch {
    overflow:scroll;
    overflow-y:scroll;
    overflow-x:hidden;
    height:390px;
}

.searchContent ul li .searchWrap:focus, .searchContent ul li .searchWrap:active {
    background: rgb(119,210,255);
    background: -moz-linear-gradient(top,  rgba(119,210,255,1) 1%, rgba(44,154,208,1) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(119,210,255,1)), color-stop(100%,rgba(44,154,208,1)));
    background: -webkit-linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
    background: -o-linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
    background: -ms-linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
    background: linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#77d2ff', endColorstr='#2c9ad0',GradientType=0 ); 
}
.searchContent ul li .searchWrap {
    padding: 10px;
    
}
.searchContent ul li .channel{
    min-width:100px;
    border:0 none;
    width:25%;
    padding: 0 20px;
}
.searchContent ul li .program{
    border:0 none
}

.searchContent ul li .program .programTitle{
    font-size:1.25em;
    font-family: 'HelveticaNeueLTStd55Roman';
}
.searchContent ul li .program .programDate{
    font-size:1em;
    font-family: 'HelveticaNeueLTStd45Light';
}
