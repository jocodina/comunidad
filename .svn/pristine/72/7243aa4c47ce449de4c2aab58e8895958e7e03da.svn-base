    background: url('../img/tablet/goIcon.png') no-repeat 95% center, -webkit-linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
    background: url('../img/tablet/goIcon.png') no-repeat 95% center, -o-linear-gradient(top,  rgba(119,210,255,1) 1%,rgba(44,154,208,1) 100%);
