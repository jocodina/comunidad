#guiaContent{
    width:75%;
    float:left;
    -webkit-box-shadow: inset 20px 10px 30px 0px rgba(0, 0, 0, 0.3);
    box-shadow: inset 6px 10px 30px 0px rgba(0, 0, 0, 0.3);
}

.guiaHeader{
    height:40px;
    background: #1F1F1F;
    overflow:hidden;
    -webkit-box-shadow: inset 20px 10px 30px 0px rgba(0, 0, 0, 0.3);
    box-shadow: inset 6px 10px 30px 0px rgba(0, 0, 0, 0.3);
    padding: 25px;
    border-bottom: 1px solid #000;
}

.selectionDate{
    float:left;
    width:25%;
    position:relative;
    min-width: 150px;
}
.horarioGuia{
    float:right;
}

#calendario {
    position: absolute;
    top:0;
    right:0;
}

.horarioGuia span{
    display:inline-block;
    color:#999999;
    font-family: 'HelveticaNeueLTStd65Medium';
}
#mainGuiaCont {
    overflow:scroll; 
    overflow-y:scroll; 
    overflow-x:hidden;
    position: relative;
}
#mainGuiaCont ul li:first-child {
    border-top:1px solid #292929;
}

#mainGuiaCont ul li a{
    display: block;
    overflow:hidden;
}
#mainGuiaCont ul li a:focus, #mainGuiaCont ul li a:active{
    background:#4f4f4f;
}
ul li .channel{
    width: 20%;
    overflow:hidden;
    border-right:1px solid #000;
    float:left;
    padding: 10px 25px 20px 25px;
    min-width:180px;
    height: 76px;
}
ul li a .program{
    width: 93%;
    float:left;
    position:relative;
    border-left:1px solid #4a4a4a;
    padding: 10px 2% 20px 2%;
    height:75px;
}
.channel .channelData{
    float:left;
    width:60%;
}

.channel .channelTitle, .channel .channelNum {
    display:block;
    color:#fff;
    line-height: 18px;
}

.channel img{
    float: left;
    margin-right: 10px;
    max-width: 50px;
}

.program .programTitle{
    color:#fff;
    width:55%;
    display:block; 
}

.searchWrap .program .programTitle{
    width:95%;
}

.programMeta {
    position: absolute;
    top: 13px;
