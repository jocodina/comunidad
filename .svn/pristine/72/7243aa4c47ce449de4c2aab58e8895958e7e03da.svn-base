/*------------------------------------------------------------------------------- ETIQUETAS-*/
h1 {
   font-family: 'NeoSans';
   color:#4CB88C;
   font-size:1.5em;
   display:inline-block;
   font-weight: normal;
}
h2 {
    font-family:'NeoSans';
    font-size:1.375em;
    display:block;
    color: #fff;
    font-weight: nbold;
}

h3 {
    font-family: 'HelveticaNeueLTStd75Bold';
    font-size: 0.875em;
    color:#4DB88C;
}

span {
    font-family:'HelveticaNeueLTStd45Light';
    font-size:1em;
    color:#4CB88C;
    display:block;
    line-height:24px;
}

span.clasificacion{
    text-align: center;
    border-radius: 10px;
    -webkit-border-radius: 35px;
    -moz-border-radius: 10px;
    -o-border-radius: 10px;
    color: white;
    font-family: 'HelveticaNeueLTStd75Bold';
    padding: 5px 16px;
    text-transform: uppercase;
    display: inline-block;
    margin: 0 10px 0 5px;
    background: rgba(0,0,0, 0.2);
    font-size:1em;
}

h1 span.clasificacion{
    font-size:0.7em;
}

span.dia, span.hora, span.canal, span.onDemand {
    font-size: 0.875em;
    color: #2C99D0;
}

span.dia {
    font-family: 'HelveticaNeueLTStd65Medium';
}

span.hora {
    font-family:'HelveticaNeueLTStd45Light';
    text-align:center;
}

span.canal, span.onDemand {
    color: #fff;
    font-family:'HelveticaNeueLTStd45Light';
}

span.onDemand {
    width:60%;
}

p {
   font-family:'HelveticaNeueLTStd45Light';
   color:#fff;
   line-height: 24px;
   font-size: 1em;
   margin: 15px 0;
   display: block;
}

strong {
   font-family: 'HelveticaNeueLTStd75Bold';
   font-weight: normal;
}
/*------------------------------------------------------------------------------- SCROLLBAR-*/
.scrollbar {
    position: absolute;
    z-index: 9999;
    right: 2px;
    width:5px;
    opacity: 0.3;
    top:30px;
    height:150px;
    overflow:hidden;
    padding:0 2px;
}

.scroll {
    position:relative;
    width:100%;
    background-color:#CCC;
    transform:  translate3d(0, 0, 0);
    top:0;
    border-radius:50px;
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    -o-border-radius: 50px;
}

/*------------------------------------------------------------------------------- HEADER-*/

header {
    background: #000;
    overflow: visible !important;
}

#primaryHead {
    height: 65px;
    position:relative;
    background: url('../img/tablet/backLine.jpg') repeat-x bottom;
}

#primaryHead .logo{
    position:absolute;
    left:20px;
}

#primaryHead ul{
    position:absolute;
    right:20px;
    top: 10px;
}
#primaryHead ul li {
    display:block;
    float:left;
}
#primaryHead ul li a{
    display: block;
    padding: 0 10px 0 0;
    text-indent: -999999px;
    color: transparent;
    width: 32px;
    height: 32px;
}

#primaryHead ul li a.active{
    height: 40px;
}

#primaryHead ul li.searchButon a{
    background: url('../img/tablet/lupa.jpg') no-repeat;
}

/*.svg #primaryHead ul li.searchButon a{
    background: url('../img/tablet/lupa.svg') no-repeat;
}*/

#primaryHead ul li.ajustes a{
    background: url('../img/tablet/ajustes.jpg') no-repeat center;
}

/*.svg #primaryHead ul li.ajustes a{
    background: url('../img/tablet/ajustes.svg') no-repeat center;
}*/

#primaryHead ul li.ajustes a.active {
    background: url('../img/tablet/ajustesActive.jpg') no-repeat center #222;
}

/*.svg #primaryHead ul li.ajustes a.active {
    background: url('../img/tablet/ajustes.svg') no-repeat center #222;
}*/

#primaryHead ul.ajustesList{
    width:250px;
    font-size: 1.5em;
    background:#222;
    position: absolute;
    top: 40px;
    right:0px;
    font-family: 'HelveticaNeueLTStd77BdCn';
    -webkit-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.5);
    -moz-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.5);
    -o-box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.5);
    box-shadow: 0px 5px 10px 0px rgba(0, 0, 0, 0.5);
    text-shadow: 0px -1px 1px rgba(0, 0, 0, 0.5);
    display:none;
    z-index:999;
}

#primaryHead ul.ajustesList li {
    float:none;     
}

#primaryHead ul.ajustesList li a{
    background:none;
    text-indent: 0;
    padding:20px;
    color: #fff;
    height:auto;
    width: auto;
}

#nav {
    width: 100%;
    overflow:hidden;
    background: rgb(31,31,31);
    background: -moz-linear-gradient(top,  rgba(31,31,31,1) 0%, rgba(38,38,38,1) 54%, rgba(31,31,31,1) 56%, rgba(38,38,38,1) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(31,31,31,1)), color-stop(54%,rgba(38,38,38,1)), color-stop(56%,rgba(31,31,31,1)), color-stop(100%,rgba(38,38,38,1)));
    background: -webkit-linear-gradient(top,  rgba(31,31,31,1) 0%,rgba(38,38,38,1) 54%,rgba(31,31,31,1) 56%,rgba(38,38,38,1) 100%);
    background: -o-linear-gradient(top,  rgba(31,31,31,1) 0%,rgba(38,38,38,1) 54%,rgba(31,31,31,1) 56%,rgba(38,38,38,1) 100%);
    background: -ms-linear-gradient(top,  rgba(31,31,31,1) 0%,rgba(38,38,38,1) 54%,rgba(31,31,31,1) 56%,rgba(38,38,38,1) 100%);
    background: linear-gradient(top,  rgba(31,31,31,1) 0%,rgba(38,38,38,1) 54%,rgba(31,31,31,1) 56%,rgba(38,38,38,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1f1f1f', endColorstr='#262626',GradientType=0 );
    height:65px;
    position:relative;
}
#nav ul li {
    display:block;
    float:left
}
#nav ul li a{
    padding: 20px;
    display:block;
    color: #fff;
    font-size: 1.5em;
    text-transform: uppercase;
    font-family:'HelveticaNeueLTStd77BdCn';
    text-shadow: 0px -1px 1px rgba(0,0,0,0.5);
    filter: dropshadow(color=#000, offx=0, offy=-1);
    min-width:65px;
    text-align: center;
}

#nav ul li a.current{
    color: #2C9AD0;
    background: url('../img/tablet/currentBack.png') no-repeat bottom center;
}

