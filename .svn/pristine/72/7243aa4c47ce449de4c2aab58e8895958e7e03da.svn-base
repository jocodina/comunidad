#infoFicha, #infoFichaCompleta {
    background: #222;
    border-radius: 10px;
    -webkit-border-radius:10px;
    -moz-border-radius:10px;
    -o-border-radius:10px;
    border-top:1px solid #000;
    border-bottom:1px solid #292929;
    overflow:hidden;
    position: relative;
}

.closeFicha {
    position: absolute;
    right: 10px;
    top: 5px;
    color:transparent;
    background: url('../img/tablet/closeBtn.png') no-repeat;
    width: 36px;
    height: 36px;
    display: block;
}

.searchContent .closeFicha{
    right:-20px;
    top:-20px;
}

.metaFicha {
    width:100%;
    overflow:scroll; 
    overflow-y:scroll; 
    overflow-x:hidden;
    position:relative;
    border-radius: 10px 0 0 10px;
    -webkit-border-radius:10px 0 0 10px;
    -moz-border-radius:10px 0 0 10px;
    -o-border-radius:10px 0 0 10px;
    height: 150px;
}

.relativeWrapp.fichaDescription {
    position:relative;
    width:65%;
    padding:2.5%;
    background: #1E1E1E;
    height: 150px;
    float:left;
    border-right:1px solid #000;
}

#infoFichaCompleta .relativeWrapp{
    max-height: 700px;
    min-height: 400px;
}


.sociales {
    background: rgb(119,210,255);
    background: -moz-linear-gradient(top,  rgba(119,210,255,1) 0%, rgba(44,154,208,1) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(119,210,255,1)), color-stop(100%,rgba(44,154,208,1)));
    background: -webkit-linear-gradient(top,  rgba(119,210,255,1) 0%,rgba(44,154,208,1) 100%);
    background: -o-linear-gradient(top,  rgba(119,210,255,1) 0%,rgba(44,154,208,1) 100%);
    background: -ms-linear-gradient(top,  rgba(119,210,255,1) 0%,rgba(44,154,208,1) 100%);
    background: linear-gradient(top,  rgba(119,210,255,1) 0%,rgba(44,154,208,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#77d2ff', endColorstr='#2c9ad0',GradientType=0 );
    overflow: hidden;
    position:absolute;
    right: 0px;
    top: 0px;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -o-border-radius: 5px;
    -moz-border-raidus: 5px;
}

.sociales li {
    display: block;
    float:left;
    border-left:1px solid #499DC7;
    border-right:1px solid #62BFED;
    width:50px;
    height:30px;
}

.sociales li input[type="checkbox"]{
    padding:0;
    margin:0;
    width: 50px;
    height: 30px;
    position: static;
    border-radius: 0 5px 5px 0;
    -webkit-border-radius: 0 5px 5px 0;
    -o-border-radius: 0 5px 5px 0;
    -moz-border-raidus: 0 5px 5px 0;
}

.sociales li img{
    display:block;
    margin:4px auto;
}

.sociales li:first-child{
    border-left:0 none;
}

.sociales li:last-child{
    border-right:0 none;
}

.datosFicha .dataWrap{
    color: #999999;
    font-size: 1em;
    line-height: 20px;
    
}

.datosFicha .dataWrap .resultDato{
    color: #2C99D0;
    display:inline-block;
}

#horariosFicha {
    width: 100%;
    height: 150px;
    overflow: hidden;
    overflow-y:scroll;
}

.relativeWrapp.fichaHorarios {
    float:left;
    position:relative;
    width: 24%;
    padding: 2.5%;
    height: 150px;
    float:left;

}

#horariosFicha h3{
    text-transform: uppercase;
}

#horariosFicha ul{
    display:block; 
    margin-top:10px;
}

#horariosFicha ul li{
    display:block; 
    padding: 5px 0;
    border-bottom:1px solid #141414;
    border-top:1px solid #2f2f2f;
    overflow:hidden;
}

#horariosFicha ul li:first-child{
    border-top:0 none;
}
#horariosFicha ul li:last-child{
    border-bottom:0 none;
}

#horariosFicha ul li span{
    display:inline-block;
    width: 20%;
    padding: 2%;
}

#horariosFicha ul li span:first-child {
    width: 39%;
}

#horariosFicha ul li span:last-child  {
    width: 30%;
    text-align: right;
}

#horariosFicha ul li span.onDemand{
    width: 60%; 
}


